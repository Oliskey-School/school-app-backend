PORT=5000
# Connect to Supabase via connection pooling with Supavisor.
DATABASE_URL="postgres://postgres.your-ref:password@aws-0-region.pooler.supabase.com:6543/postgres?pgbouncer=true"

# Direct connection to the database. Used for migrations.
DIRECT_URL="postgres://postgres.your-ref:password@aws-0-region.pooler.supabase.com:5432/postgres"

# JWT Secret for Supabase Auth
SUPABASE_JWT_SECRET="your-super-secret-jwt-token-from-supabase-settings"
